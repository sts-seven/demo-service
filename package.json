{
  "name": "demo-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist ./build",
    "global": "npm i -g pm2 webpack npm-run-all nodemon rimraf typescript just-wait",
    "preall": "rimraf ./dist ./node_modules",
    "all": "npm i",
    "test": "cross-env NODE_ENV=dev mocha -t 10000 --require ts-node/register --recursive ./test/**/*.ts",
    "prestart": "npm run clean",
    "start": "cross-env NODE_ENV=dev run-p webpack-w dev",
    "webpack": "webpack",
    "webpack-w": "webpack -w",
    "build": "run-s clean webpack",
    "wait": "just-wait -p ./dist/app.js  -t 60",
    "nodemon": "nodemon ./dist/app.js -w ./dist --on-change-only",
    "dev": "run-s wait nodemon",
    "server": "run-s wait nodemon",
    "release": "cross-env NODE_ENV=prod npm run pm2",
    "pm2": "cross-env-shell \"pm2 start ./dist/app.js\" --name \"${npm_package_name}\" "
  },
